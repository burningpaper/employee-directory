{
  "manifest_version": 3,
  "name": "LinkedIn PDF Saver with Filename",
  "version": "1.2",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/in/*"],
      "js": ["content.js"]
    }
  ]
}
